{
    "name": "plumtreesystems/secure-trading",
    "type": "library",
    "version":  "0.0.1",
    "description": "This is a Gateway being built to work with the Payum library",
    "keywords": ["payment"],
    "homepage": "https://payum.forma-pro.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "https://payum.forma-pro.com/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/AuthorizeNetAim/contributors"
        }
    ],
    "require": {
        "payum/core": "^1.5",
        "securetrading/stpp_json": "^1.0"
    },
    "require-dev": {
        "payum/core": "^1.5@dev",
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "^7.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "PlumTreeSystems\\SecureTrading": "" }
    },
    "scripts": {
        "run-unit-tests": "php bin/phpunit"
    },
    "target-dir": "PlumTreeSystems/SecureTrading",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
